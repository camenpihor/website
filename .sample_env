DARKSKY_SECRET_KEY=<SECRET>

FLASK_SKIP_DOTENV=1
FLASK_APP=api/app.py
FLASK_ENV=development

BACKEND_HOST=127.0.0.1
BACKEND_PORT=5000
# any variables to be used in Vue, must be prepended by "VUE_APP_"
VUE_APP_BACKEND_ENDPOINT=http://${BACKEND_HOST}:${BACKEND_PORT}/api
VUE_APP_MAPBOX_SECRET_TOKEN=<SECRET>
VUE_APP_WEATHERMAP_SECRET_TOKEN=<SECRET>

FRONTEND_HOST=127.0.0.1
FRONTEND_PORT=8000
FRONTEND_ADDRESS=http://${FRONTEND_HOST}:${FRONTEND_PORT}


DATABASE_URL=postgres://<user>:<pass>@<host>:<port>/<db_name>
TEST_DATABASE_URL=postgres://<user>:<pass>@<host>:<port>/<db_name>